' Gambas class file

Inherits ToolsBase
Public Const GENDER As String = ("SaveAs")

Public Function Start(Optional ElemToBuild As Variant, Optional _mode As Integer = 0) As Boolean
 
    Dim xf As String[]
    Dim f As String

    Dialog.SaveFile()
    f = Dialog.Path
    If File.Ext(f) <> "xml" Then
        f = f & ".xml"
    Endif
    cadSave.SaveToFile(f) ' <--- Tincho
    Me.Finish

End
