Private Function AddBlock(Points As Float[], Block As String) As Entity[]

  Dim newBlock As New Block

  newBlock.entities = New Entity[]
  newBlock.name = block
  newBlock.x = Points[0]
  newBlock.y = Points[1]
  gcd.CurrentDrawing.arrBlocks.Add(newBlock)

  Return newBlock.entities

End

