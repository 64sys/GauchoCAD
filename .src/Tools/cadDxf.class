  ' block record
  For i = 0 To gcd.CurrentDrawing.arrBlocks.Max
    Print #hFIle, "  0"
    Print #hFIle, "BLOCK_RECORD"
    Print #hFIle, "  5"                   ' handle
    Print #hFIle, Str$(i)               ' "4E"
    Print #hFIle, "330"
    Print #hFIle, "1"
    Print #hFIle, "100"
