Create Static
Public Const Gender As String = "Dxf"

Public t As Float = Timer

Public fp As File
Private hFile As File

Public lpCode As String
Public lpValue As String

Private eExports As Boolean

Public cEntitiesUnread As Collection
Public nEntitiesUnread As Integer
Public nEntitiesRead As Integer

Private Sub ReadData()

  Line Input #fp, lpcode
  Line Input #fp, lpValue

  If Right(lpcode, 1) = gb.Cr Then lpcode = Left(lpcode, -1)
  If Right(lpvalue, 1) = gb.cr Then lpvalue = Left(lpvalue, -1)

  lpcode = Trim$(lpcode)
  lpvalue = Trim$(lpvalue)

End

Public Function LoadFile(sFile As String, arrToFill As Entity[], Optional IgnoreLayers As Boolean = False, IgnoreBlocks As Boolean = False, IgnoreData As Boolean = False)

  Dim t As Float = Timer

  fp = Open sFile For Read

  If Not fp Then Error.Raise("Error !")
  cEntitiesUnread = New Collection
  nEntitiesUnread = 0
  nEntitiesRead = 0

  While Not Eof(fp)
    'Wait 0.0001
    ReadData
    If lpCode = "0" And lpValue = "SECTION" Then
      ' vemos que seccion es
      ReadData
      If lpCode = "2" And lpValue = "HEADER" And Not IgnoreData Then dxfLoad1Headers

      If lpCode = "2" And lpValue = "CLASSES" Then Print "DXF: CLASSES ignoradas"

      If lpCode = "2" And lpValue = "TABLES" And Not IgnoreLayers Then dxfLoad3Tables

      If lpCode = "2" And lpValue = "BLOCKS" And Not IgnoreBlocks Then dxfLoad4Blocks

      If lpCode = "2" And lpValue = "ENTITIES" Then dxfLoad5Entities(arrToFill)

      If lpCode = "2" And lpValue = "OBJECTS" Then dxfLoad6Objects

    Endif
  Wend

  Print "DXF: Leidas", nEntitiesread, " entidades"
  If cEntitiesUnread.Count > 0 Then
    Print "DXF: Un total de", nEntitiesUnread, " entidades no pudieron ser leidas:"
    For Each unread As String In cEntitiesUnread
      Print unread
    Next
  Endif
  Print
  Print "DXF: fin lectura en ", Timer - t

End

Private Sub dxfLoad6Objects()

End

Private Sub dxfLoad5Entities(arrAcumulador As Entity[])

  Dim sClave As String[]
  Dim sValue As String[]
  Dim sEntidad As String
  Dim clsHandler As Object
  Dim eNueva As Entity
  Dim Reads As Boolean

  Do
    'Debug lpcode, lpvalue
    sClave = New String[]

    sValue = New String[]

    If lpValue = "ENTITIES" Then ReadData

    sEntidad = lpValue

    If lpValue = "ENDSEC" Then Return

    ' Leo descentralizadamente las entidades
    ReadData

    'If sEntidad = "HATCH" Then Stop
    While (lpcode <> "0") And Not Eof(fp)

      sClave.Add(lpCode)           ' el codigo es el tipo de variable
      sValue.Add(lpValue)

      ReadData

    Wend

    If sEntidad = "ENDBLK" Or sEntidad = "" Then Return

    ' Termino la lectura de la entidad, busco a ver si la tengo lista
    Try clsHandler = Object.New("cad" & sEntidad)  ' obtenemos el tipo de entidad y buscamos su clase. Si no

    If IsNull(clsHandler) Then
      ' Acumulo informacion para mostrar al usuario
      If cEntitiesUnread.Exist(sEntidad) Then
        nEntitiesUnread += 1
      Else
        cEntitiesUnread.Add(sEntidad, sEntidad)
      Endif

      Continue

    End If

    Reads = False

    Try Reads = clsHandler.DXF_reading

    If Reads Then                  ' verifico si la clase ya lee DXF

      eNueva = New Entity                           ' creo la entidad

      eNueva.ClassHandler = clsHandler

      dxfReadCommonEntityData(eNueva, sClave, sValue)

      clsHandler.importDXF(eNueva, sClave, sValue)  ' la leo

      clsHandler.finish(eNueva)                     ' la termino

      arrAcumulador.Add(eNueva)                     ' la acumulo en el array corresppondiente

      'Debug eNueva.gender

      clsHandler = Null

      nEntitiesRead += 1

    Else

      ' Acumulo informacion para mostrar al usuario
      If cEntitiesUnread.Exist(sEntidad) Then
        nEntitiesUnread += 1
      Else
        cEntitiesUnread.Add(sEntidad, sEntidad)
      Endif

    Endif

  Loop

End

Private Sub dxfLoad4Blocks()

  Dim mBlock As Block
  Dim unread As Variant

  ReadData
  Do

    mBlock = New Block
    If lpCode = "0" And lpValue = "ENDSEC" Then Break

    If (lpcode = "0") And (lpvalue = "BLOCK") Then
      ReadData

      While (lpcode <> "0") And Not Eof(fp)     ' con el codigo 0 termina el encabezado del bloque
        ' siguen las entidades o el ENDBLK
        'Debug lpcode, lpvalue
        'If lpcode = "5" Then mBlock.handle = lpvalue               ' Handle
        If lpcode = "8" Then mBlock.layer = lpvalue                ' Layer
        'If lpcode = "6" Then mBlock.linetype = lpvalue             ' LineType
        'If lpcode = "62" Then mBlock.bColor = CInt(lpvalue)        ' Color code (no es RBG) (256=ByLayer)
        'If lpcode = "370" Then mBlock.LineWt = CInt(lpvalue)        ' Color code (no es RBG) (256=ByLayer)
        If lpcode = "2" Then mBlock.name = lpValue
        If lpcode = "10" Then mBlock.x = CFloat(lpValue)         ' X
        If lpcode = "20" Then mBlock.y = CFloat(lpValue)         ' Y
        ReadData

      Wend
      ' si estoy leyendo bloques, significa que estoy abriendo un plano
      mBlock.entities = New Entity[]

      gcd.CurrentDrawing.arrBlocks.Add(mBlock)

      dxfLoad5Entities(gcd.CurrentDrawing.arrBlocks.Last.entities)

    Endif
  Loop

  Print "DXF: Leidos", gcd.CurrentDrawing.arrBlocks.Count, " bloques"

End

Private Sub dxfLoad3Tables()

  '    0
  ' TABLE
  ' 2
  ' VPORT
  '   5
  '   8
  ' 330
  '   0
  ' 100
  ' AcDbSymbolTable
  ' 70
  ' 1
  '   0
  ' VPORT

  ' Yo usare dos colecciones
  Dim mLayerCount As Integer
  Dim newLayer As Layer
  Dim cLayer As Collection
  Dim jscLayers As JSONCollection

  ReadData
  Do
    If Eof(fp) Then Break

    If lpCode = "0" And lpValue = "ENDSEC" Then Break
    Select lpValue ' VIEWPORT , LAYER, etc
      Case "LAYER"

        If lpvalue = "LAYER" Then

          ' primero me fijo si existe
          If Not gcd.CurrentDrawing.cModel.Exist("LAYERS") Then
            jscLayers = New JSONCollection
            gcd.CurrentDrawing.cModel.Add(jscLayers, "LAYERS")
          Endif
          newlayer = New Layer

          newlayer.id = CStr(mLayerCount)
          ReadData
          While (lpcode <> "0") And Not Eof(fp)
            'Debug lpcode, lpvalue
            If lpcode = "5" Then newlayer.handle = lpvalue
            If lpcode = "2" Then newlayer.Name = lpvalue

            If lpcode = "6" Then newLayer.LType = lpvalue
            If lpcode = "62" Then
              newLayer.Colour = Abs(CInt(lpvalue))
              If CInt(lpvalue) > 0 Then newLayer.Shown = True
            End If
            ' If lpcode = "70" Then
            '   If ((CInt(lpvalue) And 2) = 2 Or (CInt(lpvalue) And 1) = 1) Then newLayer.Add(True, "locked") Else newLayer.Add(False, "locked")
            ' End If
            ' If lpcode = "20" Then
            '   If ((CInt(lpvalue) And 4) = 4) Then newLayer.Add(True, "frozen") Else newLayer.Add(False, "frozen")
            ' End If
            '
            ' If lpcode = "290" Then newLayer.Add(lpvalue, "print")
            ' If lpcode = "370" Then newLayer.Add(lpvalue, "width")
            ReadData
          Wend

          If newLayer.Name = "" Then newLayer.Name = newlayer.id
          gcd.CurrentDrawing.Layers.Add(newLayer, newlayer.id)
          ' DWG
          cLayer = New Collection

          cLayer.Add("LAYER", "object")
          cLayer.Add(gcd.CurrentDrawing.cModel["LAYERS"].Count, "index")
          cLayer.Add(51, "type")
          cLayer.Add([0, CInt(newlayer.id)], "handle")
          cLayer.Add(25, "size")
          cLayer.Add(120, "bitsize")
          cLayer.Add([10, 1, 12, 18], "ownerhandle")
          cLayer.Add([3, 2, 1762, 1762], "xdicobjhandle")
          cLayer.Add(newLayer.Name, "name")
          cLayer.Add(1, "is_xref_ref")
          cLayer.Add(0, "is_xref_resolved")
          cLayer.Add(0, "is_xref_dep")
          cLayer.Add([5, 0, 0, 0], "xref")
          cLayer.Add(1008, "flag")
          cLayer.Add(newLayer.LineWt, "linewt")
          cLayer.Add(newlayer.Colour, "color")
          cLayer.Add([5, 1, 7, 7], "plotstyle")
          cLayer.Add([5, 1, 10, 10], "ltype")
          gcd.CurrentDrawing.cModel["LAYERS"].Add(clayer, cLayer["handle"][1])
          Inc mLayerCount

        End If

    End Select

    ReadData
  Loop

  'gcd.UpdateLayers

  Try Print "DXF: Leidas", gcd.CurrentDrawing.cModel["LAYERS"].count, " tablas"

End

Private Sub dxfLoad1Headers()

  ' Los header se guardan asi
  '   9                     Indica que es una variable
  ' $EXTMAX                 Nombre de la variable
  '  10                     Tipo de dato1
  ' 198.0411690635561       Dato1
  '  20                     Tipo de dato2
  ' 178.7767572407179       Dato2
  '  30                     etc
  '   0

  ' Yo usare dos colecciones

  Dim sVarName As String
  Dim sClave As String[]
  Dim sValue As String[]

  ReadData
  Do
    sClave = New String[]
    sValue = New String[]

    If lpCode = "0" And lpValue = "ENDSEC" Then Break

    If lpcode = "9" Then
      sVarName = lpValue
      Do                          ' este bucle es por si la variable es un array
        ReadData
        If lpcode = "0" Or lpCode = "9" Then Break
        sClave.Add(lpCode)           ' el codigo es el tipo de variable
        sValue.Add(lpValue)
      Loop

    Endif

    If sValue.Count > 1 Then

      gcd.CurrentDrawing.cHeader.Add(sValue, sVarName)
      gcd.CurrentDrawing.cHeaderCodes.Add(sClave, sVarName)
    Else
      gcd.CurrentDrawing.cHeader.Add(sValue[0], sVarName)
      gcd.CurrentDrawing.cHeaderCodes.Add(sClave[0], sVarName)

    End If
  Loop

  Print "DXF: Leidas", gcd.CurrentDrawing.cHeader.Count, "variables de ambiente"

End

Private Sub dxfReadCommonEntityData(e As Entity, sCode As String[], sValue As String[])

  Dim i As Integer
  Dim lay As Layer

  For i = 0 To sValue.Max

    If sCode[i] = "5" Then e.handle = sValue[i]             ' Handle
    If sCode[i] = "8" Then
      ' obtengo el id basado en el nombre
      For Each Lay In gcd.CurrentDrawing.layers
        If Lay.Name = sValue[i] Then
          e.LayerId = lay.id              ' Layer
          Break
        End If
      Next
    End If
    If sCode[i] = "6" Then e.LineType = 0 'sValue[i]           ' LineType
    If sCode[i] = "62" Then e.colour = CInt(sValue[i])      ' Color code (no es RBG) (256=ByLayer)
    If sCode[i] = "370" Then e.LineWidth = 1 ' CInt(sValue[i])  ' debemos ir a una tabla para ver

    e.Gender = e.ClassHandler.gender

  Next

End

Public Function SaveFile(sName As String) As Integer

  hFile = Open sName For Write Create

  If dxfSave1HeadersAndVars() Then Return False
  If dxfSave2Tables() Then Return False
  If dxfSave3BlockRecord() Then Return False
  If dxfSave4Blocks() Then Return False
  If dxfSave5Entities() Then Return False
  If dxfSave6Objects() Then Return False

  hFile.Close

End

