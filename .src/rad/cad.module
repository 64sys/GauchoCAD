'' Input a DXF color return a RGB color.

Public Function GauchoDecodeColor() As Integer[]

  Dim rgb As New Integer[]

  Dim s As String
  Dim r As String
  Dim g As String
  Dim b As String
  Dim irgb As Integer
  Dim rgbx As String[]

  rgbx = DWGColors()

  For Each s In rgbx
    r = Split(s, ",")[0]
    g = Split(s, ",")[1]
    b = Split(s, ",")[2]
    irgb = Color.RGB(CInt(r), CInt(g), CInt(b))
    rgb.Add(irgb)
  Next

  Return rgb

End

