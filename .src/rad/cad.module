  Dim sCont As String
  Dim cTmp As New Collection
  Dim cDeco As New Collection
  Dim sLay As String
  Dim slx As String[]
  Dim sTag As String
  Dim sVal As String
  Dim j As Integer
  Dim code As Collection

  code = DWGDimCodes()

  sCont = Replace(sItem, "\t", " ")
