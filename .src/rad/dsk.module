'' Replace a color in an svg file with one that contrasts with the background of the desktop theme

Public Function Contrary(sFile As String, sColor As String) As String

  Dim svg As String

  If Application.DarkTheme Then
    svg = Replace(File.Load(sFile), sColor & ";", "#eeeeee;")
  Else
    svg = Replace(File.Load(sFile), sColor & ";", "#000000;")
  Endif

  Return svg

End

Public Function Mimex(strMimeType As String) As String[] ''Devuelve una lista de los programas que estan asociados al tipo mime que se pasa como parametro.

  Dim stxOut As New String[]
  Dim df As DesktopFile

  For Each df In DesktopFile.FromMime(strMimeType)
    stxOut.Add(df.ProgramName)
  Next

  Return stxOut

End

'' Devuelve el valor en pixeles de una cadena de texto, teniendo en cuenta la tipografía
'' Ejemplo:
'' Dim i As Integer
'' i = TextWidth(TextBox1.Text, TextBox1.Font.Name, TextBox1.Font.Size)

Public Function TextWidth(str As String, sFont As String, zSize As Integer) As Integer

  Dim fnt As New Font

  fnt.Name = sFont
  fnt.Size = zSize

  Return fnt.TextWidth(str)

End

Public Function MimeAppPicture(strApp As String, intSize As Integer) As Picture '' Devuelve la pintura "picture" de una aplicación.

  Dim pic As Picture

  If DesktopFile[strApp] Then
    pic = DesktopFile[strApp].GetIcon(22).Picture
  Else
    pic = Picture["icon:/" & CStr(intSize) & "/remove"]
  Endif

  Return pic

End

'' Returns a circular image with the color indicated as a parameter

Public Function ImageDot(xColor As Integer) As Image

  Dim hColor As ColorInfo
  Dim img As Image
  'Dim D As Integer

  hColor = Color[Color.ButtonBackground]

  img = New Image(128, 128)

  Paint.Begin(img)

  Paint.MoveTo(64, 64)

  Paint.Rectangle(0, 0, 128, 128)
  Paint.Stroke

  Paint.Arc(64, 64, 60, 0, 2 * Pi)
  Paint.ClosePath

  Paint.LineWidth = 0

  Paint.LineWidth = 10

  Paint.Brush = Paint.Color(Color.RGB(0, 0, 0, 255))

  'Paint.Brush = Paint.Color(Color.Blue)
  If hColor.Luminance > 160 Then
    Paint.Brush = Paint.Color(Color.Black)
  Else
    Paint.Brush = Paint.Color(Color.White)
  Endif

  Paint.Brush = Paint.Color(xColor)
  Paint.Fill(True)

  If hColor.Luminance > 160 Then
    Paint.Brush = Paint.Color(Color.Black)
  Else
    Paint.Brush = Paint.Color(Color.White)
  Endif

  Paint.Stroke

  Paint.End

  Return img

End

'' Return the same image but with a frame

Public Function ImageFrame(img As Image) As Image

  'Dim lt As Variant
  'Dim img As Image

  'For Each lt In Starter.cColors
  Dim hColor As ColorInfo

  hColor = Color[Color.ButtonBackground]

  Paint.Begin(img)
  Paint.NewPath

  If hColor.Luminance > 160 Then
    Paint.Brush = Paint.Color(Color.Black)
  Else
    Paint.Brush = Paint.Color(Color.White)
  Endif

  '  Paint.Brush = Paint.Color(Color.Black)

  Paint.LineWidth = 2
  Paint.MoveTo(1, 1)
  Paint.LineTo(img.W - 1, 1)
  Paint.LineTo(img.W - 1, img.H - 1)
  Paint.LineTo(1, img.H - 1)
  Paint.LineTo(1, 1)
  Paint.LineTo(img.W - 1, 1)

  Paint.Stroke
  Paint.End
  'Next
  Return img

End

Public Function FileChooser(Optional strInputPath As String, Optional stxExt As String[]) As String[] ''Selecciona la ruta completa de un archivo, con el nombre y las extensiones. Como opcional se puede pasar un directorio que es a donde se dirigira el filechooser cuando se abra. También como opcional se pued epasar un filtro de tipos de archivos separados por :, por ejemolo "txt:csv"

  Dim strExt As String
  Dim strFilterA As String
  Dim strFilterB As String
  Dim stxFilter As New String[]
  Dim stxFilepaths As New String[] ' Para el caso de seleccion de multiples archivos

  If strInputPath = "" Then
    strInputPath = User.Home
  Endif

  Dialog.Title = ("Select File")
  Dialog.Filter = Null

  If stxExt Then
    If stxExt.Count > 0 Then
      For Each strExt In stxExt
        stxFilter.Add("*." & String.LCase(strExt) & ";" & "*." & String.UCase(strExt))
      Next
      strFilterA = stxFilter.Join(";")
      strFilterB = ("Filter") & ": " & stxExt.Join(",")
      Dialog.Filter = [strFilterA, strFilterB]
    Endif
  Endif

  If Not Dialog.OpenFile(True) Then
    stxFilepaths = Dialog.Paths
  Endif
  Return stxFilepaths

End

Public Function DirChooser(Optional stInputPath As String) As String  ''Selecciona la ruta de un directorio mediante una caja de dialogo.

  Dim stDirPath As String

  If stInputPath = "" Then
    stInputPath = User.Home
  Else
    Dialog.Path = stInputPath
  Endif

  Dialog.Title = ("Select directory")
  Dialog.Filter = Null
  Dialog.SelectDirectory

  stDirPath = Dialog.Path

  Return stDirPath

End

Public Sub SendEmail(stxAddreses As String[], stxNames As String[], Optional strMode As String, Optional strSubject As String, Optional strBody As String, Optional strAttachment As String)

  Dim intR As Integer
  Dim strAddresses As String
  Dim strNames As String

  For intR = 0 To stxAddreses.Max

    If intR = 0 Then
      strAddresses = stxAddreses[intR]
    Else
      strAddresses = strAddresses & "; " & stxAddreses[intR]
    Endif
  Next

  For intR = 0 To stxNames.Max

    Select intR
      Case 0
        strNames = stxNames[intR]

      Case stxNames.Max
        strNames &= " ~ " & stxNames[intR]

      Case Else
        strNames &= " ~ " & stxNames[intR]
    End Select

  Next

  If strMode = "" Then
    strMode = "to"
  Else
    strMode = String.LCase(strMode)
  Endif

  If strSubject = "" Then
    strSubject = ""
  Endif

  If strAttachment = "" Then
    strAttachment = ""
  Endif

  If strBody = "" Then
    strBody = ("Hi") & " " & strNames & "."
  Endif

  Desktop.SendMail(stxAddreses,,, strSubject, strBody)

End

